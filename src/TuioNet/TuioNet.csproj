<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>TuioNet</PackageId>
        <Version>1.0.0</Version>
        <Authors>Erich Querner, Giles Coope</Authors>
        <Company>Interactive Scape GmbH</Company>
        <Description>TuioNet is a .Net implementation of the TUIO specification by Martin Kaltenbrunner. It supports TUIO 1.1 and TUIO 2.0 over UDP and Websocket.</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/InteractiveScapeGmbH/TuioNet</PackageProjectUrl>
        <RepositoryUrl>https://github.com/InteractiveScapeGmbH/TuioNet.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>TUIO Transport NETStandard Touch Tangible Multitouch</PackageTags>
        
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <TargetFrameworks>net6.0;net8.0;netstandard2.1;netstandard2.0</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
    </ItemGroup>
</Project>
